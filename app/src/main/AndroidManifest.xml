<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.itzs.testipc">

    <permission android:name="com.itzs.testipc.permission.ACCESS_BOOK_SERVICE"
        android:protectionLevel="normal"/>

    <uses-permission android:name="com.itzs.testipc.permission.ACCESS_BOOK_SERVICE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".MessengerService"
            android:process=":remote" />
        <service
            android:name=".BookManagerService"
            android:process=":bookManager" />

        <activity android:name=".MessengerActivity" />
        <activity android:name=".BookManagerActivity"></activity>
    </application>

</manifest>
